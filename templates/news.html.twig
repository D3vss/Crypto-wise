{% extends "base.html.twig" %}
{% block title %}CryptoWise - Home
{% endblock %}

{% block body %}

<div class="container">
{% for i in 0..25 %}
    <div class="row">
        <div class="col">
            <div class="card"> 
                <div class="card-header">{{news[i].username}}</div>
                <div class="card-body" >
                    <p>{{news[i].status}}</p>
                </div>
            </div>
        </div>
        <div class="card col"><div class="card"></div></div>
        <div class="card col"><div class="card"></div></div>
    </div>
{% endfor %}
</div>

{% endblock %}